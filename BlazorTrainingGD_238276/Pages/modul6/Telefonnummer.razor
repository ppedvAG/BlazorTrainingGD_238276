<input @bind="Nummerintern" @bind:event="oninput" type="range" min="1" max="99999999" class="form-range" />
@Nummerintern
@code {
    private int _Nummerintern;
    public int Nummerintern
    {
        get { return _Nummerintern; }
        set
        {
            _Nummerintern = value;
            Nummer = value;
            NummerChanged.InvokeAsync(value);
        }
    }
  [Parameter]
    public int Nummer { get; set; }
    [Parameter]
    public EventCallback<int> NummerChanged { get; set; }
}
